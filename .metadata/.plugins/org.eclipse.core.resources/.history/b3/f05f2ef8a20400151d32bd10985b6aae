import java.io.File;
import java.util.*;

public class WikiCodePuller{

	public static void main(String[] args){
		String line = "";
		File file = new File("");

		try{
			Scanner sc = new Scanner(file);
			
		}catch(Exception e){
			e.printStackTrace();
		}

		boolean isCode = false;

		while(file.hasNextLine()){
			line = line+file.nextLine();
		}

		System.out.println(line);

		/*
